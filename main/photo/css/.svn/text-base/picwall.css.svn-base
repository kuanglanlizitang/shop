.tabli {
	position: relative;
	border: 0px solid black;
	height: 80px;
	border-radius: 2px;
	text-align: center;
	width: 23%;
	float: left;
	list-style: none;
	margin: 1% 1%;
	-webkit-flex: 1;
	flex: 1;
}

.tabdiv {
	position: relative;
	border: 0px solid black;
	height: 80px;
	border-radius: 2px;
	box-shadow: #CCCCCC;
	text-align: center;
	width: 21%;
	float: left;
	list-style: none;
	-webkit-flex: 1;
	margin-right: 5px;
	flex: 1;
}


.tabimg {
	width: 100%;
	height: 80px;
	float: left;
	margin: 0 auto;
	max-width: 100%;
	max-height: 100px;
}

.tabimg_wall {
	width: 100%;
