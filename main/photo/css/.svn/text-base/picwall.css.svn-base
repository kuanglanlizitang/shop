}

.tabname {
	position: absolute;
	bottom: 0px;
	height: 20px;
	width: 100%;
	text-align: center;
	background: rgba(0, 0, 0, 0.6);
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000);
	color: #fff;
	font-size: 12px;
	padding: 3px 0;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}

.bigdiv {
	position: fixed;
